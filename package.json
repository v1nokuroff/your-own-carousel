{
    "name": "my-app",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://v1nokuroff.github.io/your-own-carousel",
    "dependencies": {
        "@types/node": "^16.18.44",
        "@types/react": "^18.2.21",
